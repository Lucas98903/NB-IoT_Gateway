   "source": [
    "import struct\n",
    "def IEEE754_Hex_To_Float(data_in):\n",
    "    data_first = int(\"0x\" + data_in, 16)\n",
    "    data_middle = struct.pack(\"q\", data_first)\n",
    "    data_out = struct.unpack(\">f\", data_middle[0:4])[0]\n",
    "    return data_out"
   ]
  },
